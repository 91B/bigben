<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="itemDM" class="com.walmart.gmp.ingestion.platform.framework.data.core.DataManager">
        <constructor-arg value="file://item.json"/>
    </bean>
    <bean id="feedDM" class="com.walmart.gmp.ingestion.platform.framework.data.core.DataManager">
        <constructor-arg value="file://feed.json"/>
    </bean>
    <bean id="itemIndexDM" class="com.walmart.gmp.ingestion.platform.framework.data.core.DataManager">
        <constructor-arg value="file://item_index.json"/>
    </bean>

    <bean class="com.walmart.gmp.feeds.FeedStatusAndCountsChecker">
        <constructor-arg>
            <map>
                <entry key="item" value-ref="itemDM"/>
                <entry key="feed" value-ref="feedDM"/>
                <entry key="item_index" value-ref="itemIndexDM"/>
            </map>
        </constructor-arg>
    </bean>
</beans>