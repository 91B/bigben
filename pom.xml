<?xml version="1.0" encoding="UTF-8"?><project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
  <modelVersion>4.0.0</modelVersion>
  <groupId>com.walmartlabs.components</groupId>
  <artifactId>dis-evt-sch</artifactId>
  <packaging>pom</packaging>
  <version>0.5-SNAPSHOT</version>
  <modules>
    <module>lib</module>
    <module>app</module>
  </modules>
  <name>Distributed Event Scheduler</name>
  <url>http://maven.apache.org</url>
  <parent>
    <groupId>com.walmart.platform</groupId>
    <artifactId>platform-parent-pom</artifactId>
    <version>14.3.8</version>
  </parent>
  <properties>
    <ignite.version>1.5.0.final</ignite.version>
    <gmp-ingestion-platform.version>1.73</gmp-ingestion-platform.version>
    <hazelcast.version>3.6.3</hazelcast.version>
    <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
    <ingestion-pipeline-messages.version>0.7</ingestion-pipeline-messages.version>
  </properties>
  <dependencyManagement>
    <dependencies>
      <dependency>
        <groupId>com.hazelcast</groupId>
        <artifactId>hazelcast</artifactId>
        <version>${hazelcast.version}</version>
      </dependency>
      <dependency>
        <groupId>com.hazelcast</groupId>
        <artifactId>hazelcast-all</artifactId>
        <version>${hazelcast.version}</version>
      </dependency>
      <dependency>
        <groupId>com.walmartlabs.components</groupId>
        <artifactId>event-scheduler-lib</artifactId>
        <version>0.5-SNAPSHOT</version>
      </dependency>
      <dependency>
        <groupId>org.apache.ignite</groupId>
        <artifactId>ignite-core</artifactId>
        <version>${ignite.version}</version>
      </dependency>
      <dependency>
        <groupId>org.apache.ignite</groupId>
        <artifactId>ignite-spring</artifactId>
        <version>${ignite.version}</version>
      </dependency>
      <dependency>
        <groupId>org.apache.ignite</groupId>
        <artifactId>ignite-log4j</artifactId>
        <version>${ignite.version}</version>
      </dependency>
      <dependency>
        <groupId>com.walmart.services.gmp</groupId>
        <artifactId>ingestion-pipeline-messages</artifactId>
        <exclusions>
          <exclusion>
            <groupId>com.walmart.catalog</groupId>
            <artifactId>catalog-services-api</artifactId>
          </exclusion>
          <exclusion>
            <groupId>com.walmart.catalog.canonical</groupId>
            <artifactId>canonical-api</artifactId>
          </exclusion>
          <exclusion>
            <groupId>com.walmart.catalog.supplyitem</groupId>
            <artifactId>supply-item-services-api</artifactId>
          </exclusion>
        </exclusions>
        <version>${ingestion-pipeline-messages.version}</version>
      </dependency>
      <dependency>
        <groupId>com.walmart.services</groupId>
        <artifactId>gmp-ingestion-platform</artifactId>
        <version>${gmp-ingestion-platform.version}</version>
        <exclusions>
          <exclusion>
            <groupId>com.hazelcast</groupId>
            <artifactId>hazelcast</artifactId>
          </exclusion>
          <exclusion>
            <groupId>com.hazelcast</groupId>
            <artifactId>hazelcast-all</artifactId>
          </exclusion>
        </exclusions>
      </dependency>
      <dependency>
        <groupId>junit</groupId>
        <artifactId>junit</artifactId>
        <version>3.8.1</version>
        <scope>test</scope>
      </dependency>
    </dependencies>
  </dependencyManagement>
  <build>
    <plugins>
      <plugin>
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-enforcer-plugin</artifactId>
        <version>1.0.0-walmart-01</version>
        <executions>
          <execution>
            <id>enforce-banned-dependencies</id>
            <goals>
              <goal>enforce</goal>
            </goals>
            <configuration>
              <rules>
                <bannedDependencies>
                  <excludesUrl>http://gec-maven-nexus.walmart.com/nexus/content/sites/tools-repo/com/walmart/platform/devtools/BlacklistedArtifacts.txt</excludesUrl>
                </bannedDependencies>
              </rules>
              <fail>true</fail>
            </configuration>
          </execution>
        </executions>
      </plugin>
      <plugin>
        <groupId>org.codehaus.mojo</groupId>
        <artifactId>cobertura-maven-plugin</artifactId>
        <version>2.7</version>
        <configuration>
          <formats>
            <format>xml</format>
          </formats>
        </configuration>
        <executions>
          <execution>
            <phase>package</phase>
            <goals>
              <goal>cobertura</goal>
            </goals>
          </execution>
        </executions>
      </plugin>
      <plugin>
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-compiler-plugin</artifactId>
        <version>3.5.1</version>
        <configuration>
          <source>1.8</source>
          <target>1.8</target>
        </configuration>
      </plugin>
      <plugin>
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-javadoc-plugin</artifactId>
        <version>2.10.1</version>
        <configuration>
          <javadocExecutable>${java.home}/../bin/javadoc</javadocExecutable>
        </configuration>
        <executions>
          <execution>
            <id>attach-javadocs</id>
            <goals>
              <goal>jar</goal>
            </goals>
            <configuration>
              <additionalparam>-Xdoclint:none</additionalparam>
            </configuration>
          </execution>
        </executions>
      </plugin>
      <plugin>
        <artifactId>maven-surefire-plugin</artifactId>
        <configuration>
          <systemPropertyVariables>
            <buildDirectory>${project.basedir}</buildDirectory>
          </systemPropertyVariables>
          <argLine>-Xms1G -Xmx2G</argLine>
        </configuration>
      </plugin>
      <plugin>
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-dependency-plugin</artifactId>
        <version>${apache.maven-dependency-plugin.version}</version>
        <executions>
          <execution>
            <id>install</id>
            <phase>install</phase>
            <goals>
              <goal>analyze-only</goal>
            </goals>
          </execution>
        </executions>
      </plugin>
      <plugin>
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-release-plugin</artifactId>
        <version>2.5.2</version>
      </plugin>
      <plugin>
        <groupId>org.codehaus.mojo</groupId>
        <artifactId>exec-maven-plugin</artifactId>
        <version>1.4.0</version>
        <executions>
          <execution>
            <goals>
              <goal>exec</goal>
            </goals>
          </execution>
        </executions>
        <configuration>
          <executable>maven</executable>
        </configuration>
      </plugin>
    </plugins>
  </build>
  <pluginRepositories>
    <pluginRepository>
      <id>nexus_plugin_release</id>
      <url>http://gec-maven-nexus.walmart.com/nexus/content/groups/public/</url>
      <releases>
        <enabled>true</enabled>
      </releases>
      <snapshots>
        <enabled>true</enabled>
      </snapshots>
    </pluginRepository>
  </pluginRepositories>
</project>
